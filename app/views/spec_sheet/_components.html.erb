<% components = data %>
<td class="image pt3">
  <div class="img" style="background-image:url('<%= project.image_url %>');"></div>
</td>
<td class="content">
  <table class="rows">
    <colgroup>
      <col width="100%">
    </colgroup>
    <% components.each do |component| %>
      <%= render partial: "spec_sheet/component", locals: { component: component } %>
    <% end %>
  </table>
</td>