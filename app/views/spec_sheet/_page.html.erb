<div class="page">
  <table class="main">
    <tr>
      <td colspan="2" class="header">
        <%= render partial: 'spec_sheet/header', locals: { title: title, project: project } %>
      </td>
    </tr>
    <tr class="body">
      <td class="image pt3">
        <div class="img" style="background-image:url('<%= project.images.first.file.url(:large) %>');"></div>
      </td>
      <td class="content">
        <%= render partial: "spec_sheet/#{view}", locals: { data: data } %>
      </td>
    </tr>
  </table>
</div>
